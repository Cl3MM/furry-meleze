.row
  .col-md-12
    %label
      Dénomination usuelle du déchet :
    = DechetDenomination[ebsdd.dechet_denomination][3..-1]
    %br

  .col-md-12
    %label
      Consistance :
    - if ebsdd.dechet_consistance == 0
      Solide
    - elsif ebsdd.dechet_consistance == 1
      Liquide
    - else
      Gazeux
