<table class="table table-striped">
  <thead>
    <tr>
      <% @content.shift.each do | header |%>
        <th><%= header %></th>
      <% end %>
    </tr>
  </thead>
  <tbody>
  <% @content.each do | row | %>
    <tr>
      <% row.each do | col | %>
        <td><%= col %></td>
      <% end %>
      </tr>
    <% end %>
  </tbody>
</table>
