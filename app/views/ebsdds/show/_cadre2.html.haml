      %dd
        = ebsdd.destinataire.cp
      %dt
        Ville
      %dd
        = ebsdd.destinataire.ville
      %dt
        Téléphone
      %dd
        = ebsdd.destinataire.tel
      %dt
        Fax
      %dd
        = ebsdd.destinataire.fax
      %dt
        Responsable
      %dd
        = ebsdd.destinataire.responsable
      %dt
